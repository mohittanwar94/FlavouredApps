<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ConcentAppDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="windowNoTitle">true</item>
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="AppTheme.AdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">8sp</item>
        <item name="android:text">@string/ad_attribution</item>
        <item name="android:background">#A6A094</item>
        <item name="android:width">15dp</item>
        <item name="android:height">15dp</item>
    </style>

    <style name="CustomShapeAppearanceBottomSheetDialog" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">16dp</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="AppMaterialTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowAnimationStyle">@style/CustomActivityAnimation</item>
        <item name="checkboxStyle">@style/MyCheckBox</item>
        <item name="radioButtonStyle">@style/Widget.App.RadioButton</item>
        <item name="android:navigationBarColor">@color/white</item>
    </style>

    <style name="AppDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="MyCustomTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/_12sdp</item>
    </style>

    <style name="MyCheckBox" parent="@style/Widget.MaterialComponents.CompoundButton.CheckBox">
        <item name="materialThemeOverlay">@style/ThemeOverlay.CheckBox</item>
    </style>

    <style name="ThemeOverlay.CheckBox" parent="">
        <item name="colorSecondary">@color/black</item>
        <item name="colorAccent">@color/black</item>
        <item name="colorOnSurface">@color/white</item>
    </style>

    <style name="NewCheckBox" parent="Widget.AppCompat.CompoundButton.CheckBox">
        <item name="colorAccent">@color/white</item>
        <item name="colorPrimary">@color/white</item>
        <item name="android:textColorSecondary">@color/greyish_brown</item>
        <item name="colorSecondary">@color/white</item>
        <item name="tickColorInactive">@color/white</item>
        <item name="colorControlActivated">@color/white</item>
    </style>

    <style name="custom_style_dialog">
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="TabTextAppearance" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="DialogTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="ShapeAppearance.Button" parent="ShapeAppearance.MaterialComponents">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">@dimen/_10sdp</item>
        <item name="cornerSizeTopRight">@dimen/_10sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_10sdp</item>
        <item name="cornerSizeBottomLeft">@dimen/_10sdp</item>
    </style>

    <style name="BottomSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/bottomSheetStyleWrapper</item>
    </style>

    <style name="bottomSheetStyleWrapper" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <string name="ad_attribution">Ad</string>

    <style name="Theme.AppCompat.Translucent" parent="Theme.AppCompat.NoActionBar">
        <item name="android:background">#77000000
        </item> <!-- Or any transparency or color you need -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <style name="ConsentBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:layout_gravity">bottom</item>
    </style>

    <style name="SplashTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_background</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
    </style>

